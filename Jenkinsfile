pipeline {
    agent any

    stages {
        
		stage('Git Clone') {
            steps {
                echo 'git clonening'
            }
        }
		stage('Build') {
            steps {
                echo 'Build code'
            }
        }
		stage('Deployment') {
            steps {
                echo 'Deploy Code'
            }
        }
		stage('Test') {
            steps {
                echo 'Test code'
            }
        }
		stage('Release') {
            steps {
                echo 'release code'
            }
        }
    }
}
